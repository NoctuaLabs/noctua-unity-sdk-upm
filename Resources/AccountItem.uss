.account-item {
    background-color: #2D3138;
    height: 40px;
    border-radius: 10px;
    flex-direction: row;
    align-items: center;
    margin-top: 3px;
    margin-bottom: 3px;
    padding: 5px;
    -unity-text-align: middle-left;
}


.account-item-player-name {
    margin-left: 10px;
    font-size: 14px;
}

.account-item-spacing {
    flex: 1;
}

.account-item-recent-label {
    font-size: 10px;
}

