.list-container {
    flex-direction: column;
    align-items: stretch;
    align-self: stretch;
    flex-grow: 1;
}

.account-selection-info {
    flex-wrap: wrap;
    top: 10px;
    width: 187px;
    margin-bottom: 5px;
    font-size: 12px;
    white-space: normal;
    align-self: flex-start;
}

.account-selection-list {
    border-color: transparent;
    margin-top: 1px;
    margin-bottom: 1px;
    align-self: stretch;
    align-items: stretch;
}

.separator {
    flex-direction: row;
    align-items: center;
    height: 24px;
    justify-content: space-between;
    align-self: stretch;
}

.separator-label {
    font-size: 10px;
    color: #8B8E91;
    -unity-text-align: middle-center;
}

.continue-other-button {
    height: 40px;
    bottom: 0;
    border-color: #E5E7EB;
    border-width: 1px;
    border-radius: 10px;
    background-color: rgb(31, 35, 39);
    align-self: stretch;
}