    align-items: auto;
    background-position: center;
}

.dialog-content {
    padding: 14px;
    margin-top: 15px;
    flex-direction: column;
    width: 300px;
}

.dialog-footer {
    position: absolute;
    bottom: 0;
    padding: 14px;
    width: 300px;
    margin-top: 10px;
    margin-bottom: 10px;
    align-items: center;
    justify-content: center;
}

.dialog-err-label {
