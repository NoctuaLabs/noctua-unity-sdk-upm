.base {
    background-color: rgb(31, 35, 39);
    color: white;
    position: absolute;
}

.root {
    position: absolute;
    width: 100%;
    height: 100%;
    justify-content: center;
}

.hide {
    display: none;
}

.show {
    display: flex;
}

.err-box {
    padding-top: 10px;
    padding-right: 0;
    padding-bottom: 10px;
    padding-left: 0;
    width: 100%;
    height: 16px;
    align-items: center;
    justify-content: center;
}

.err-label {
    color: red;
    font-size: 10px;
    -unity-text-align: upper-left;
    white-space: normal;
    overflow: visible;
    flex-wrap: wrap;
    padding: 2px 0;
    margin: 0;
    width: 100%;
    max-width: 100%;
}

.unity-scroller--vertical {
    width: 4px;
    background-color: rgb(45, 49, 56);
}

.dialog-box {
    flex-direction: column;
    border-radius: 20px;
    border-color: rgba(165, 165, 165, 0.5);
    width: 319px;
    height: 337px;
    border-width: 1px;
    align-items: center;
    padding: 14px;
    align-self: center;
    transition-duration: 10s;
    transition-property: position;
    transition-timing-function: ease-in;
    position: absolute;
}

.dialog-box-keyboard-shown {
    flex-direction: column;
    border-radius: 20px;
    border-color: rgba(165, 165, 165, 0.5);
    width: 319px;
    height: 337px;
    border-width: 1px;
    align-items: center;
    padding: 14px;
    align-self: center;
    position: absolute;
    top: 12px;
}

.dialog-header {
    height: 24px;
    flex-direction: row;
    font-size: 12px;
    align-items: center;
    align-self: flex-start;
}

.dialog-title {
    margin-top: 0;
    font-size: 12px;
}

.dialog-back-button {
    margin-top: 0;
    margin-right: 14px;
    height: 24px;
    width: 24px;
    flex-direction: column;
    border-width: 0;
    border-color: transparent;
    background-color: transparent;
    justify-content: flex-start;
    background-image: url("project://database/Packages/com.noctuagames.sdk/Resources/BackIcon.png?fileID=21300000&guid=26917d39f45fdac47a6e498ce7306fd4&type=3#BackIcon");
    align-items: auto;
    background-position: center;
}

.dialog-close-button {
    margin-top: 0;
