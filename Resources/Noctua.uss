.noctua-text-input {
    position: relative;
    flex-direction: column;
    margin-right: -10px;
    margin-left: -10px;
}

.unity-base-text-field__input {
    border-radius: 10px;
    margin-bottom: 10px;
    border-color: rgb(71, 75, 78);
    --unity-cursor-color: white;
}

.unity-base-text-field__input:hover {
    border-radius: 10px;
    margin-bottom: 10px;
    border-color: white;
    --unity-cursor-color: white;
}

.unity-base-text-field__input:active {
    border-radius: 10px;
    margin-bottom: 10px;
    border-color: white;
    --unity-cursor-color: white;
}

.unity-base-field__label {
    color: grey;
    margin-top: -2px;
}

.unity-list-view__empty-label {
    display: none;
}

.unity-list-view__item:hover {
    background-color: transparent;
}

.unity-list-view__item:active {
    background-color: transparent;
}

.unity-list-view__item:focus {
    background-color: transparent;
}

.noctua-text-input #unity-text-input {
    padding-left: 10px;
    padding-right: 10px;
    height: 40px;
    margin-left: 10px;
    margin-right: 10px;
    background-color: transparent;
}

Label {
    border-width: 0;
}

Label:hover {
    border-width: 0;
}

.noctua-text-input Label {
    position: absolute;
    top: 12px;
    left: 10px;
    font-size: 12px;
    padding-left: 10px;
    background-color: transparent;
    border-width: 0;
}

.custom-textfield-input-code #unity-text-input {
    position: relative;
    flex-direction: column;
    background-color: transparent;
    width: 37px;
    height: 47px;
    -unity-text-align: middle-center;
    font-size: 14px;
    border-width: 1px;
    border-color: rgb(71, 75, 78);
    border-radius: 6px;
}

.general-dropdown-box {
    margin-bottom: 10px;
    margin-right: -0.3px;
    margin-left: -0.3px;
}

.general-dropdown-box .unity-base-field__label {
    display: none;
}

.general-dropdown-box .unity-base-field__input {
    background-color: transparent;
    border-color: rgb(71, 75, 78);
    border-radius: 8px;
    margin-right: 0;
    margin-left: 0;
    height: 40px;
}

.general-dropdown-box .unity-base-popup-field__arrow {
    width: 10px;
    -unity-background-image-tint-color: rgb(98, 100, 104);
}

.btn-password-show {
    background-image: resource('Eye-Close');
    top: 9.5px;
    right: 10px;
    position: absolute;
}

.btn-password-hide {
    background-image: resource('Eye-Open');
    top: 9.5px;
    right: 10px;
    position: absolute;
}
